	.eqv PrintChar 		11
	.eqv PrintInt 		1 
	.eqv PrintString	4 
	.eqv ReadChar		12
	.eqv ReadInt 		5
	.eqv ReadString		8
	.eqv Exit		10 
.data 
	.eqv maxLine, 81 # espace pour chaque ligne. 80 + 1 pour '\0'
l00:	.space maxLine
l01:	.space maxLine
l02:	.space maxLine
l03:	.space maxLine
l04:	.space maxLine
l05:	.space maxLine
l06:	.space maxLine
l07:	.space maxLine
l08:	.space maxLine
l09:	.space maxLine
l10:	.space maxLine
l11:	.space maxLine
l12:	.space maxLine
l13:	.space maxLine
l14:	.space maxLine
l15:	.space maxLine
l16:	.space maxLine
l17:	.space maxLine
l18:	.space maxLine
l19:	.space maxLine
	.eqv maxLines, 20
lignes:	.dword l00, l01, l02, l03, l04, l05
	.dword l06, l07, l08, l09, l10, l11
	.dword l12, l13, l14, l15, l16, l17, l18, l19


.text 


exit : 
	li a7, 10
	ecall
